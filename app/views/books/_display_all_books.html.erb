<div class="">
  <h2>Books</h2>
  <ul>
    <% books.each do |book| %>
      <li>
        <div class="">
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', class: " profile-img", fallback: "no_image.jpg" %>
        </div>
        <%= link_to book_path(book) do %>
          <%= book.title %>
        <% end %>
        <%= book.body %>
      </li>
    <% end %>
  </ul>
</div>
